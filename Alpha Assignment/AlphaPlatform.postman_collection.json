{
  "info": {
    "_postman_id": "alpha-platform-collection",
    "name": "Alpha Platform API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get People",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/people",
          "host": ["{{base_url}}"],
          "path": ["people"]
        }
      }
    },
    {
      "name": "Enrich Person",
      "request": {
        "method": "POST",
        "header": [],
        "url": {
          "raw": "{{base_url}}/enrich/:person_id",
          "host": ["{{base_url}}"],
          "path": ["enrich", ":person_id"]
        }
      }
    },
    {
      "name": "Get Context Snippet",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/context-snippets/:companyId",
          "host": ["{{base_url}}"],
          "path": ["context-snippets", ":companyId"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000"
    }
  ]
}